<Window x:Class="WpfApplication1.SinglePlayerProperties"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="SinglePlayerProperties" Height="300" Width="300" Closing="Window_Closing">
    <StackPanel>
        <local:ChooseMaze x:Name="myMaze" Height="190" Margin="10 10 10 10" Padding="10 10 10 10"/>
        <Button x:Name="button" Content="Start" HorizontalAlignment="Center" Padding="10 10 10 10" Click ="Start_Game"/>

    </StackPanel>


</Window>
